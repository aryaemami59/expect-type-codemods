{
  "name": "expect-type-codemods",
  "version": "0.0.1",
  "description": "A collection of codemods for the expect-type library",
  "dependencies": {
    "globby": "^14.1.0",
    "jscodeshift": "^17.1.2",
    "prettier": "^3.5.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@eslint/js": "^9.21.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jscodeshift": "^0.12.0",
    "@types/node": "^22.13.6",
    "@vitest/eslint-plugin": "^1.1.36",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "expect-type": "^1.2.0",
    "jiti": "^2.4.2",
    "publint": "^0.3.7",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "vitest": "^3.0.7"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "bun": {
        "types": "./dist/index.d.ts",
        "default": "./src/index.ts"
      },
      "module-sync": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "module": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": "./dist/bin.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup --config=$INIT_CWD/tsup.config.mts",
    "check-exports": "attw --pack $INIT_CWD",
    "check-package-json": "publint --strict $INIT_CWD",
    "clean": "rimraf -g $INIT_CWD/'{dist,coverage,?(.)temp}'/",
    "format-check": "prettier -c .",
    "format": "prettier -w .",
    "lint-fix": "eslint --fix $INIT_CWD",
    "lint": "eslint $INIT_CWD",
    "prepack": "yarn clean && yarn build",
    "test-types": "tsc -p $INIT_CWD/tsconfig.json",
    "test": "vitest --run --config=$INIT_CWD/vitest.config.mts"
  },
  "engines": {
    "node": ">= 16"
  },
  "packageManager": "yarn@4.6.0",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
